{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "baseUrl": "./",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "removeComments": false
  }
}